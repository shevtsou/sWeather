{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
  ],
  "plugins": [
    [
      "@babel/transform-runtime",
      {
        "regenerator": true,
      },
      [["import", {
        "libraryName": "antd",
        "style": true,   // or 'css'
      }]],
    ],
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "transform-es2015-modules-commonjs",
        "babel-plugin-dynamic-import-node",
      ],
    },
  },
}